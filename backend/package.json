{
  "name": "guild-sys-backend",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npx prisma generate && npx prisma migrate deploy && tsc",
    "lint": "eslint src/**/*.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:seed": "ts-node ./scripts/seed.ts"
  },
  "dependencies": {
    "@microsoft/signalr": "^10.0.0",
    "@prisma/client": "^5.0.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "node-fetch": "^2.6.7",
    "socket.io": "^4.8.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.19.11",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^8.57.1",
    "globals": "^17.3.0",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.55.0"
  }
}